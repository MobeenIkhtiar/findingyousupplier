@using FindingYouSupplier.Helping_Classes
<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="">
<!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Finding You Suppliers</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="apple-touch-icon.png">
    @{Html.RenderAction("cssCache", "Home");}

</head>
<body>
    <div class='preloader'><div class='loaded'>&nbsp;</div></div>
    <!-- Sections -->
    <section id="social" class="social">
        <div class="row">
            <div class="container">
                <div class="col-md-3 col-xs-12 col-sm-3">
                    <a href="mailto:support@findingyousuppliers.com" style="color:white;"><i class="fa fa-envelope" aria-hidden="true"></i><strong> support@findingyousuppliers.com</strong></a>
                </div>
                <div class="col-md-3 col-sm-3">

                </div>
                @{
                    SessionDTO s = new SessionDTO();
                }
                @if (s.getName() == null)
                {
                    <div class="col-md-3 col-xs-12 col-sm-3" id="top-bar">
                        <i class="fa fa-user" style="color: white;"></i><span style="color: white;">&nbsp;<strong>Trade Supplier</strong></span><a href="@Url.Action("Login", "Auth", new { Id = StringCipher.Base64Encode("1"), err = "" })">&nbsp; Sign In /</a><a href="@Url.Action("Packages", "Home", new { Id = StringCipher.Base64Encode("1") })">Register</a>

                    </div>
                    <div class="col-md-3 col-xs-12 col-sm-3" id="top-bar">
                        <i class="fa fa-user" style="color: white;"></i><span style="color: white;">&nbsp;<strong>Buyer / Distributer</strong></span><a href="@Url.Action("Login", "Auth", new { Id = StringCipher.Base64Encode("0"), err = "" })">&nbsp; Sign In /</a><a href="@Url.Action("Packages", "Home", new { Id = StringCipher.Base64Encode("0") })">Register</a>

                    </div>
                }
                else
                {
                    <div class="col-md-offset-4 col-md-2 col-sm-3 col-xs-12">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="color: white;"><strong>@s.getName()</strong><i class="caret"></i></a>
                        <ul class="dropdown-menu">
                            @if (s.getRole() == 3)
                            {
                                <li><a href="@Url.Action("Index","Admin")">Admin Panel</a></li>
                            }
                            <li><a href="@Url.Action("Index", "Account")">My Account</a></li>
                            <li><a href="@Url.Action("Logout", "Auth")">Log Out</a></li>
                        </ul>

                    </div>
                }
            </div>
        </div>
    </section>
    <nav class="navbar navbar-default">
        <div class="container">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index","Home")"><img src="~/Content/Images/logo.png" class="img-responsive" alt="Logo" width="150" height="100" /></a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">

                <ul class="nav navbar-nav navbar-right">
                    <li><a href="@Url.Action("Index","Home")">Home</a></li>
                    @if (s.getName() == null)
                    {
                        <li><a href="@Url.Action("GetStarted", "Home")">Get Started</a></li>
                    }
                    <li><a href="@Url.Action("UKSuppliers","Home")">UK Suppliers</a></li>
                    <li><a href="@Url.Action("InternationalSuppliers","Home")">International Suppliers</a></li>
                    <li><a href="@Url.Action("Help","Home")">Help</a></li>
                </ul>

            </div><!-- /.navbar-collapse -->

        </div><!-- /.container-fluid -->
    </nav>

    @RenderBody()

    <br /><br />
    <section class="footer footer-bg">
        <div class="container-fluid">
            <div class="row text-center">
                <div class="col-md-4 col-sm-4 col-xs-3 footer-widget">
                    <h5><strong>About Us</strong></h5>
                    <ul>
                        <li><a href="@Url.Action("Index","Home")">Home</a></li>
                        <li><a href="@Url.Action("GetStarted", "Home")">Get Started</a></li>
                        <li><a href="@Url.Action("UKSuppliers","Home")">UK Suppliers</a></li>
                        <li><a href="@Url.Action("InternationalSuppliers","Home")">International Suppliers</a></li>
                        <li><a href="#">Help</a></li>
                    </ul>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-3 footer-widget">
                    <h5><strong>Our Services</strong></h5>
                    <ul>
                        <li><a href="@Url.Action("Packages","Home", new { Id = StringCipher.Base64Encode("0") })">Entrepreneur</a></li>
                        <li><a href="@Url.Action("Packages","Home", new { Id = StringCipher.Base64Encode("1") })">UK Supplier</a></li>
                        <li><a href="@Url.Action("Packages","Home", new { Id = StringCipher.Base64Encode("1") })">International Supplier</a></li>
                        <li><a href="@Url.Action("Privacy", "Home")">Privacy Policy</a></li>
                        <li><a href="@Url.Action("Terms","Home")">Terms & Conditions</a></li>
                    </ul>
                </div>
                <div class="col-md-4 col-sm-4 col-xs-6 footer-widget">
                    <h5><strong>Contact Us</strong></h5>
                    <ul>
                        <li><a href="#"><i class="fa fa-envelope" aria-hidden="true"> support@findingyousuppliers.com</i></a></li>
                        <li><a href="https://www.instagram.com/findingyousuppliers/" target="_blank"><i class="fa fa-instagram" aria-hidden="true"> Instagram</i></a></li>
                        <li><a href="https://www.facebook.com/pg/Finding-You-Suppliers-Ltd-1689429231144021" target="_blank"><i class="fa fa-facebook-official" aria-hidden="true"> Facebook</i></a></li>
                        <li><a href="#"><i class="fa fa-twitter-square" aria-hidden="true"> Twitter</i></a></li>
                    </ul>
                </div>
            </div>
            <hr />
            <div class="row text-center" id="cont">
                <strong style="color: white;">© 2018 FindingYouSuppliersLtd | Company Number 11175252</strong>
                @*<span style="color:white; font-weight:bold;"> Contact Us:</span> <a href="mailto:support@findingyousuppliers.com"><strong>support@findingyousuppliers.com</strong></a>*@
            </div>
        </div>
    </section>
    <script async src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.11.2.min.js"></script>
    <script async src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
 
    @{Html.RenderAction("jsCache", "Home");}
</body>

</html>
